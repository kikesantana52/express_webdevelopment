<% include partials/header %>

<h1>The post page</h1>
<ul>
    <% for(var i = 0; i < posts.length; i++) {%>
      <li>Tittle: <%= posts[i].tittle %> - <strong>Author:</strong> <%= posts[i].author %></li>
    <% } %>
</ul>

<ul>
    <% posts.forEach(function(post){ %>
      <li>Tittle: <%= post.tittle %> - <strong>Author:</strong> <%= post.author %></li>
    <% }) %>
</ul>
<% include partials/footer %>
